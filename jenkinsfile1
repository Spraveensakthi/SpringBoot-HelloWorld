pipeline{
    agent any
    environment {
        PATH = "C:/Program Files/Apache maven-3.8.2/bin: $PATH"
           }
    stages{
            
       stage('Build'){
            steps{
                bat 'mvn clean package'
            }
         }
         stage('Deploy'){
            steps{
            javaw -jar target/demo-0.0.1-SNAPSHOT.jar
            }
        }
            
        }
  }
